
/*рограммf для вычисления целочисленного логарифма по основанию 2.*/
/*Карандашев П1-17*/
/*https://stepik.org/lesson/13377/step/13?auth=registration&unit=3571*/
#include <iostream>
using namespace std;
int FOL(int k){
    int z=0, n=1;
    while(n<=k){
        n *= 2;
        z++;
    }
    return z-1;
}

int main(){
    int t;
    float x;
    cin>>t;
    for(int i=0; i<t; i++){
        cin>>x;
        x = FOL(x);
        cout<<x<<endl;
    }
    return 0;
}
